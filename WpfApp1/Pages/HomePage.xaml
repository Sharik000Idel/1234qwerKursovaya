<Page x:Class="WpfApp1.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomePage">


    <Grid>

        
        
        <Image HorizontalAlignment="Left" VerticalAlignment="Top" Width="180" Height="200" Source="/Resources/UserPhoto.png"/>

        <WrapPanel Margin="180 0 0 0" Orientation="Vertical">
            <TextBlock x:Name="NameText"/>
            <TextBlock x:Name="SurnameText" Margin="0 2 0 2"/>
            <TextBlock x:Name="Lastname"/>
            <TextBlock x:Name="EamiUserl"  Margin="0 2 0 2"/>
            <TextBlock x:Name="DateBirthUser"/>
            <TextBlock x:Name="Numbertext"  Margin="0 2 0 2"/>
        </WrapPanel>
        
        <Button HorizontalAlignment="Left" Content="Выйти" VerticalAlignment="Bottom" Height="20" Width="120" Background="Red" Margin="20" Click="Button_Click"/>

    </Grid>

    
</Page>
